{
  "name": "eslint-config-hyoban",
  "type": "module",
  "version": "2.4.0",
  "packageManager": "pnpm@9.1.0",
  "description": "Hyoban's ESLint Config",
  "author": "Stephen Zhou <hi@hyoban.cc>",
  "license": "MIT",
  "homepage": "https://github.com/hyoban/eslint-config-hyoban#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyoban/eslint-config-hyoban.git"
  },
  "bugs": "https://github.com/hyoban/eslint-config-hyoban/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunchee",
    "build:inspector": "npx @eslint/config-inspector@latest build",
    "dev": "bunchee -w",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepare": "pnpm run build",
    "prepublishOnly": "pnpm run build",
    "test": "vitest",
    "typecheck": "tsc"
  },
  "peerDependencies": {
    "eslint": "^8.56.0 || ^9.0.0",
    "typescript": ">=4.7.4"
  },
  "dependencies": {
    "@eslint-react/eslint-plugin": "^1.5.11",
    "@eslint/js": "^9.2.0",
    "@stylistic/eslint-plugin": "^2.1.0",
    "@types/eslint": "^8.56.10",
    "defu": "^6.1.4",
    "eslint-config-flat-gitignore": "^0.1.5",
    "eslint-plugin-antfu": "^2.2.0",
    "eslint-plugin-command": "^0.2.1",
    "eslint-plugin-hyoban": "^0.4.1",
    "eslint-plugin-import-x": "^0.5.0",
    "eslint-plugin-jsonc": "^2.15.1",
    "eslint-plugin-package-json": "^0.13.1",
    "eslint-plugin-react-compiler": "0.0.0-experimental-53bb89e-20240515",
    "eslint-plugin-react-hooks": "5.1.0-rc-915b914b3a-20240515",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-regexp": "^2.5.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-unicorn": "^53.0.0",
    "globals": "^15.2.0",
    "jsonc-eslint-parser": "^2.4.0",
    "typescript-eslint": "^7.9.0"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "20.12.12",
    "bunchee": "^5.1.5",
    "eslint": "^9.2.0",
    "eslint-typegen": "^0.2.4",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
