{
  "name": "eslint-config-hyoban",
  "type": "module",
  "version": "4.0.2",
  "packageManager": "pnpm@10.8.0",
  "description": "Hyoban's ESLint Config",
  "author": {
    "name": "Stephen Zhou",
    "email": "hi@hyoban.cc",
    "url": "https://hyoban.cc"
  },
  "license": "MIT",
  "homepage": "https://github.com/hyoban/eslint-config-hyoban#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyoban/eslint-config-hyoban.git"
  },
  "bugs": "https://github.com/hyoban/eslint-config-hyoban/issues",
  "keywords": [
    "eslint-config"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "scripts": {
    "build": "pnpm typegen && bunchee",
    "build:inspector": "npx @eslint/config-inspector build",
    "dev": "pnpm typegen && npx @eslint/config-inspector",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepare": "simple-git-hooks && pnpm run build",
    "prepublishOnly": "pnpm run build",
    "test": "vitest",
    "typecheck": "tsc",
    "typegen": "tsx scripts/typegen.ts"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "typescript": ">=4.8.4"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "@eslint-react/eslint-plugin": "^1.47.1",
    "@eslint/js": "^9.24.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@typescript-eslint/types": "^8.29.1",
    "@unocss/eslint-config": "^66.1.0-beta.11",
    "defu": "^6.1.4",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-plugin-antfu": "^3.1.1",
    "eslint-plugin-command": "^3.2.0",
    "eslint-plugin-hyoban": "^0.6.1",
    "eslint-plugin-import-x": "^4.10.3",
    "eslint-plugin-jsonc": "^2.20.0",
    "eslint-plugin-package-json": "^0.29.1",
    "eslint-plugin-react-compiler": "^19.0.0-beta-e993439-20250405",
    "eslint-plugin-react-google-translate": "^0.1.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "npm:@hyoban/eslint-plugin-tailwindcss@^3.18.0",
    "eslint-plugin-unicorn": "^58.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^1.1.1",
    "read-package-up": "^11.0.0",
    "typescript-eslint": "^8.29.1"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.0.2",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.1",
    "bunchee": "^6.5.0",
    "eslint": "^9.24.0",
    "eslint-typegen": "^2.1.0",
    "jiti": "^2.4.2",
    "lint-staged": "^15.5.1",
    "simple-git-hooks": "^2.12.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "unocss": "^66.1.0-beta.11",
    "vitest": "^3.1.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
