{
  "name": "eslint-config-hyoban",
  "type": "module",
  "version": "4.0.10",
  "packageManager": "pnpm@10.15.1",
  "description": "Hyoban's ESLint Config",
  "author": {
    "name": "Stephen Zhou",
    "email": "hi@hyoban.cc",
    "url": "https://hyoban.cc"
  },
  "license": "MIT",
  "homepage": "https://github.com/hyoban/eslint-config-hyoban#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyoban/eslint-config-hyoban.git"
  },
  "bugs": "https://github.com/hyoban/eslint-config-hyoban/issues",
  "keywords": [
    "eslint-config"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "scripts": {
    "build": "pnpm typegen && bunchee",
    "build:inspector": "npx @eslint/config-inspector build",
    "dev": "pnpm typegen && npx @eslint/config-inspector",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepare": "simple-git-hooks && pnpm run build",
    "prepublishOnly": "pnpm run build",
    "test": "vitest",
    "typecheck": "tsc",
    "typegen": "tsx scripts/typegen.ts"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "typescript": ">=4.8.4"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "@eslint-react/eslint-plugin": "^1.53.1",
    "@eslint/js": "^9.35.0",
    "@stylistic/eslint-plugin": "^5.3.1",
    "@typescript-eslint/types": "^8.43.0",
    "@unocss/eslint-config": "^66.5.1",
    "defu": "^6.1.4",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-plugin-antfu": "^3.1.1",
    "eslint-plugin-command": "^3.3.1",
    "eslint-plugin-hyoban": "^0.6.1",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsonc": "^2.20.1",
    "eslint-plugin-package-json": "^0.45.2",
    "eslint-plugin-react-google-translate": "^0.1.1",
    "eslint-plugin-react-hooks": "6.0.0-rc.2",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-regexp": "^2.10.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "4.0.0-beta.0",
    "eslint-plugin-unicorn": "^61.0.2",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.4.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^1.1.2",
    "read-package-up": "^11.0.0",
    "typescript-eslint": "^8.43.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.2.0",
    "@types/node": "^22.18.1",
    "@types/react": "^19.1.12",
    "bunchee": "^6.6.0",
    "eslint": "^9.35.0",
    "eslint-typegen": "^2.3.0",
    "jiti": "^2.5.1",
    "lint-staged": "^16.1.6",
    "simple-git-hooks": "^2.13.1",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "unocss": "^66.5.1",
    "vitest": "^3.2.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
