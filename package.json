{
  "name": "eslint-config-hyoban",
  "type": "module",
  "version": "2.7.1",
  "packageManager": "pnpm@9.1.2",
  "description": "Hyoban's ESLint Config",
  "author": "Stephen Zhou <hi@hyoban.cc>",
  "license": "MIT",
  "homepage": "https://github.com/hyoban/eslint-config-hyoban#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyoban/eslint-config-hyoban.git"
  },
  "bugs": "https://github.com/hyoban/eslint-config-hyoban/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm typegen && bunchee",
    "build:inspector": "npx @eslint/config-inspector@latest build",
    "dev": "pnpm typegen && npx @eslint/config-inspector@latest",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepare": "simple-git-hooks && pnpm run build",
    "prepublishOnly": "pnpm run build",
    "test": "vitest",
    "typecheck": "tsc",
    "typegen": "tsx scripts/typegen.ts"
  },
  "peerDependencies": {
    "eslint": "^8.56.0 || ^9.0.0",
    "typescript": ">=4.7.4"
  },
  "dependencies": {
    "@eslint-react/eslint-plugin": "^1.5.12",
    "@eslint/js": "^9.3.0",
    "@stylistic/eslint-plugin": "^2.1.0",
    "@types/eslint": "^8.56.10",
    "defu": "^6.1.4",
    "eslint-config-flat-gitignore": "^0.1.5",
    "eslint-plugin-antfu": "npm:@hyoban/eslint-plugin-antfu@2.2.0-2",
    "eslint-plugin-command": "^0.2.3",
    "eslint-plugin-hyoban": "^0.5.1",
    "eslint-plugin-import-x": "^0.5.0",
    "eslint-plugin-jsonc": "^2.15.1",
    "eslint-plugin-package-json": "^0.13.1",
    "eslint-plugin-react-compiler": "0.0.0-experimental-c8b3f72-20240517",
    "eslint-plugin-react-hooks": "5.1.0-rc-d3ce0d3ea9-20240520",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-unicorn": "^53.0.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "globals": "^15.3.0",
    "jsonc-eslint-parser": "^2.4.0",
    "typescript-eslint": "^7.10.0"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "20.12.12",
    "bunchee": "^5.1.5",
    "eslint": "npm:eslint-ts-patch@9.2.0-6",
    "eslint-ts-patch": "9.2.0-6",
    "eslint-typegen": "^0.2.4",
    "lint-staged": "^15.2.4",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.10.5",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
