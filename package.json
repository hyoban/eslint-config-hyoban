{
  "name": "eslint-config-hyoban",
  "type": "module",
  "version": "0.2.5",
  "packageManager": "pnpm@8.15.5",
  "description": "Hyoban's ESLint Config",
  "author": "Stephen Zhou <hi@hyoban.cc>",
  "license": "MIT",
  "homepage": "https://github.com/hyoban/eslint-config-hyoban#readme",
  "repository": "hyoban/eslint-config-hyoban",
  "bugs": "https://github.com/hyoban/eslint-config-hyoban/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": "./dist/bin/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunchee",
    "dev": "bunchee -w",
    "lint": "eslint .",
    "prepare": "simple-git-hooks && pnpm run build",
    "prepublishOnly": "pnpm run build",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc"
  },
  "peerDependencies": {
    "@eslint-react/eslint-plugin": ">=1.5.3",
    "@next/eslint-plugin-next": ">=14.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript": ">=4.7.4"
  },
  "peerDependenciesMeta": {
    "@eslint-react/eslint-plugin": {
      "optional": true
    },
    "@next/eslint-plugin-next": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^0.3.1",
    "@stylistic/eslint-plugin": "^1.7.0",
    "eslint-flat-config": "^1.1.10",
    "eslint-plugin-antfu": "^2.1.2",
    "eslint-plugin-format": "^0.1.0",
    "eslint-plugin-github": "^4.10.2",
    "eslint-plugin-hyoban": "^0.2.7",
    "eslint-plugin-import": "npm:eslint-plugin-i@^2.29.1",
    "eslint-plugin-jsonc": "^2.14.1",
    "eslint-plugin-package-json": "^0.10.4",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "prompts": "^2.4.2",
    "typescript-eslint": "^7.3.1"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "1.5.6",
    "@next/eslint-plugin-next": "^14.1.4",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/eslint": "^8.56.6",
    "@types/node": "^20.11.30",
    "@types/prompts": "^2.4.9",
    "bunchee": "^4.4.8",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "lint-staged": "^15.2.2",
    "release-it": "^17.1.1",
    "release-it-pnpm": "^4.1.3",
    "simple-git-hooks": "^2.11.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "vitest": "^1.4.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
