{
  "name": "eslint-config-hyoban",
  "type": "module",
  "version": "0.1.3",
  "packageManager": "pnpm@8.15.2",
  "description": "Hyoban's ESLint Config",
  "author": "Stephen Zhou <hi@hyoban.cc>",
  "license": "MIT",
  "homepage": "https://github.com/hyoban/eslint-config-hyoban#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyoban/eslint-config-hyoban.git"
  },
  "bugs": "https://github.com/hyoban/eslint-config-hyoban/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "pnpm run build",
    "lint": "eslint .",
    "typecheck": "tsc",
    "test": "vitest",
    "build": "bunchee",
    "dev": "bunchee -w",
    "start": "tsx src/index.ts",
    "prepublishOnly": "pnpm run build"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}"
    },
    "hooks": {
      "before:init": [
        "pnpm run lint",
        "pnpm run typecheck",
        "pnpm run test --run"
      ]
    }
  },
  "peerDependencies": {
    "eslint": "^8.56.0",
    "typescript": ">=4.7.4",
    "@eslint-react/eslint-plugin": ">=1.5.3",
    "@next/eslint-plugin-next": ">=14.1.0",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "peerDependenciesMeta": {
    "@eslint-react/eslint-plugin": {
      "optional": true
    },
    "@next/eslint-plugin-next": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^0.3.1",
    "@stylistic/eslint-plugin": "^1.6.1",
    "eslint-flat-config": "^1.1.6",
    "eslint-plugin-antfu": "^2.1.2",
    "eslint-plugin-import": "npm:eslint-plugin-i@^2.29.1",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "local-pkg": "^0.5.0",
    "prompts": "^2.4.2",
    "typescript-eslint": "^7.0.1"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "1.5.4-next.5",
    "@next/eslint-plugin-next": "^14.1.0",
    "@types/node": "^20.11.17",
    "@types/prompts": "^2.4.9",
    "bunchee": "^4.4.6",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  }
}
