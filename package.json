{
  "name": "eslint-config-hyoban",
  "type": "module",
  "version": "3.1.14",
  "packageManager": "pnpm@9.14.2",
  "description": "Hyoban's ESLint Config",
  "author": {
    "name": "Stephen Zhou",
    "email": "hi@hyoban.cc",
    "url": "https://hyoban.cc"
  },
  "license": "MIT",
  "homepage": "https://github.com/hyoban/eslint-config-hyoban#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyoban/eslint-config-hyoban.git"
  },
  "bugs": "https://github.com/hyoban/eslint-config-hyoban/issues",
  "keywords": [
    "eslint-config"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "scripts": {
    "build": "pnpm typegen && bunchee",
    "build:inspector": "npx @eslint/config-inspector build",
    "dev": "pnpm typegen && npx @eslint/config-inspector",
    "lint": "eslint --flag unstable_ts_config",
    "lint:fix": "eslint --flag unstable_ts_config --fix",
    "prepare": "simple-git-hooks && pnpm run build",
    "prepublishOnly": "pnpm run build",
    "release": "release-it",
    "test": "vitest",
    "typecheck": "tsc",
    "typegen": "tsx scripts/typegen.ts"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "typescript": ">=4.8.4"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "@eslint-react/eslint-plugin": "^1.17.1",
    "@eslint/js": "^9.15.0",
    "@stylistic/eslint-plugin": "^2.11.0",
    "@typescript-eslint/types": "^8.16.0",
    "@unocss/eslint-config": "^0.64.1",
    "defu": "^6.1.4",
    "eslint-config-flat-gitignore": "^0.3.0",
    "eslint-plugin-antfu": "^2.7.0",
    "eslint-plugin-command": "^0.2.6",
    "eslint-plugin-hyoban": "^0.6.1",
    "eslint-plugin-import-x": "^4.4.3",
    "eslint-plugin-jsonc": "^2.18.2",
    "eslint-plugin-package-json": "^0.15.6",
    "eslint-plugin-react-compiler": "^19.0.0-beta-df7b47d-20241124",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "eslint-plugin-unicorn": "^56.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.12.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.1",
    "read-package-up": "^11.0.0",
    "typescript-eslint": "^8.16.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^0.5.6",
    "@types/node": "20.17.9",
    "@types/react": "^18.3.12",
    "bunchee": "^5.6.2",
    "eslint": "^9.15.0",
    "eslint-typegen": "^0.3.2",
    "jiti": "^2.4.0",
    "lint-staged": "^15.2.10",
    "release-it": "^17.10.0",
    "release-it-pnpm": "^4.6.3",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "unocss": "^0.64.1",
    "vitest": "^2.1.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --flag unstable_ts_config --fix"
  }
}
